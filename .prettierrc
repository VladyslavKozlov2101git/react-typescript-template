{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "react",
    "clsx",
    "<THIRD_PARTY_MODULES>",
    "^./(.*)$",
    "^../(.*)$",
    "@containers/",
    "@app/",
    "@services/",
    "@pages/",
    "@components/",
    "@redux/",
    "@assets/",
    "@models/",
    "@helpers/",
    "@shared/",
    "@routes/"
  ],
  "semi": true,
  "singleQuote": true,
  "arrowParens": "always",
  "bracketSpacing": true,
  "editorconfig": true,
  "jsxSingleQuote": false,
  "printWidth": 100,
  "tabWidth": 2,
  "trailingComma": "all",
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "jsxBracketSameLine": false,
  "proseWrap": "always"
}
